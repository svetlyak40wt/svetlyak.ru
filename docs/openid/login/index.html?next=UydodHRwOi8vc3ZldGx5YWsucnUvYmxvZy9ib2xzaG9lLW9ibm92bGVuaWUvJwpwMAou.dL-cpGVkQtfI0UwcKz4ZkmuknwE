    <script type="text/javascript">
        var select_box;
        var input_field;
        var favicon;

        $(document).ready( function() {
            select_box = $('#id_openid_url_0');
            input_field = $('#id_openid_url_1');
            favicon = $('<img class="favicon" src="/media/images/favicons/openid.gif"/>');
            favicon.insertBefore(select_box);

            input_field.addClass('virgin');

            input_field.focus( function() {
                if (input_field.hasClass('virgin')) {
                    input_field.val('');
                    input_field.removeClass('virgin');
                }
            });

            select_box.change( function() {
                var opt = $("select option[value=" + select_box.val() + "]");
                var url = opt.css('background-image')

                url = url.substring(4, url.length - 1);
                if (url[0] == '"')
                    url = url.substring(1, url.length - 1);

                favicon.attr('src', url);

                if (input_field.hasClass('virgin')) {
                    if (opt.val() == 'openid') {
                        input_field.val('http://some.openid.com');
                    } else {
                        input_field.val('Ваше имя');
                    }
                }
            });
            var opt = $("select option[value='yandex']")[0];
            opt.selected = 'selected';
            select_box.change();
        });
    </script>

